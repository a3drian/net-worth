{
    "name": "net-worth-shared",
    "version": "1.0.8",
    "description": "Shared resources for NetWorth",
    "repository": {},
    "author": "a3drian",
    "license": "ISC",
	"private": true,
    "scripts": {
        "build": "node build",
        "compile": "node node_modules/typescript/bin/tsc --project tsconfig.json --outDir bin",
        "compile:rd": "rd bin /s /q && node node_modules/typescript/bin/tsc --project tsconfig.json --outDir bin",
        "compile:rm": "rm -rf bin/* && node node_modules/typescript/bin/tsc --project tsconfig.json --outDir bin",
        "start": "npm run compile -- --watch"
    },
    "devDependencies": {
        "@types/node": "^17.0.5",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.5.4"
    }
}
